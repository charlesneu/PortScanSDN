<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:odl="http://opendaylight.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:jpa="http://aries.apache.org/xmlns/jpa/v2.0.0"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v2.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"
           odl:use-default-for-reference-types="true">


    <jpa:enable/>
    <tx:enable/>

    <bean id="netsecService" class="org.opendaylight.netsec.persistence.impl.NetsecServiceImpl"/>

    <service interface="org.opendaylight.netsec.persistence.NetsecService" ref="netsecService"/>

</blueprint>
